{
  "extends": ["plugin:prettier/recommended", "plugin:react/recommended"],
  "plugins": ["prettier"],
  "parser": "babel-eslint",
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": 2,
    "react/prop-types": 0
  },
  "env": {
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}
